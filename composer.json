{
    "name": "princeton255/laravel-relying-party",
    "description": "Laravel Middleware for authorization in an OAuth2 Relying Party",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tumaini Mosha",
            "email": "princeton.mosha@gmail.com"
        }
    ],
    "require": {
        "lcobucci/jwt": "^3.2",
        "illuminate/support": "5.5 - 5.6",
        "illuminate/auth": "5.5 - 5.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "OAuth2Middleware\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
